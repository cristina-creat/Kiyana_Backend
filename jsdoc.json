{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\|node_modules|swagger-definitions|uploads|downloads|config)_",
        "include": ["controllers","services","routes","models","middlewares"],
        "exclude": ["node_modules","swagger-definitions","uploads","downloads","config"]
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "recurse": "true",
        "destination": "./docs"
    }
}
